<!DOCTYPE html>
<html>
  <head>
    <title>Hipster Habitat</title>
    <%= favicon_link_tag asset_path('tab_icon.jpg') %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <script type="text/javascript">
      window.dropdownURL = "<%= image_url('tab_icon.jpg') %>"
      window.footerURL = "<%= image_url('footer-img2.png') %>"
    </script>
    

    <link href="https://fonts.googleapis.com/css?family=Maven+Pro:700" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" />
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
